{
  "keyboard_name": "Manuform",
  "url": "https://github.com/julianschuler/keyboards/tree/master/manuform",
  "maintainer": "julianschuler",
  "manufacturer": "QMK",

  "processor": "atmega32u4",
  "bootloader": "caterina",

  "debounce": 15,
  "diode_direction": "COL2ROW",
  "matrix_pins": {
    "cols": ["D6", "B7", "B6", "B5", "B4", "E6"],
    "rows": ["D7", "C6", "D4", "D0", "F7", "F6", "F5", "F4", "F1"]
  },

  "usb": {
    "vid": "0xFEED",
    "pid": "0x6465",
    "device_version": "1.0.0",
    "polling_interval": 1,
    "force_nkro": true
  },

  "features": {
    "encoder": true,
    "extrakey": true,
    "nkro": true
  },

  "tapping": {
    "permissive_hold": true
  },

  "encoder": {
    "rotary": [
      {
        "pin_a": "D2",
        "pin_b": "D3",
        "resolution": 4
      }
    ]
  }
}
